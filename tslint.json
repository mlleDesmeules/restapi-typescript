{
	"rules" : {
		"align"                  : true,
		"class-name"             : true,
		"comment-format"         : [
			true,
			"check-space"
		],
		"indent"                 : [
			true,
			"tabs"
		],
		"max-classes-per-file"   : [
			true,
			2
		],
		"no-eval"                : true,
		"no-internal-module"     : true,
		"no-trailing-whitespace" : false,
		"no-unsafe-finally"      : true,
		"no-var-keyword"         : true,
		"one-line"               : [
			true,
			"check-whitespace"
		],
		"quotemark"              : [
			true,
			"double"
		],
		"semicolon"              : [
			true,
			"always"
		],
		"trailing-comma"         : [
			true,
			{
				"multiline"  : "always",
				"singleline" : "never"
			}
		],
		"triple-equals"          : [
			true,
			"allow-null-check"
		],
		"typedef-whitespace"     : [
			true,
			{
				"call-signature"       : "space",
				"index-signature"      : "space",
				"parameter"            : "space",
				"property-declaration" : "space",
				"variable-declaration" : "space"
			}
		],
		"variable-name"          : [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore",
			"allow-pascal-case"
		],
		"whitespace"             : [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type"
		]
	}
}